{
  "name": "tripster-client",
  "contributors": [
    "Sue Ann Ioanis <sueann.ioanis@gmail.com>",
    "Michael Yuen <iam@mikey.baby>"
  ],
  "version": "0.1.0",
  "description": "Tripster React App",
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3", 
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.4.0",
    "@types/jest": "^24.9.0",
    "@types/node": "^12.12.25",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/react-router-dom": "^5.1.3",
    "apollo-boost": "^0.4.7",
    "graphql": "^14.5.8",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.3.0",
    "react-toast-component": "^1.3.14",
    "styled-components": "^5.0.0",
    "styled-normalize": "^8.0.6",
    "typescript": "^3.7.5"
  },
  "lint-staged": {
    "*.{ts,tsx,json,css,scss,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "rules": {
      "prettier/prettier": "error"
    },
    "extends": [
      "react-app",
      "plugin:prettier/recommended",
      "prettier/react"
    ],
    "plugins": [
      "prettier"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^5.3.7",
    "@storybook/addon-links": "^5.3.7",
    "@storybook/addons": "^5.3.7",
    "@storybook/react": "^5.3.7",
    "husky": "^4.2.1",
    "lint-staged": "^10.0.2",
    "prettier": "^1.19.1"
  },
  "keywords": [
    "tripster",
    "trip",
    "travel",
    "react",
    "ssr"
  ],
  "license": "MIT"
}
